#mobile-menu .cross-btn {
    position: absolute;
    display: block;
    width: 30px;
    height: 30px;
    right: 5px;
    top: 2px;
    z-index: 10;
}
	#mobile-menu .cross-btn:before, #mobile-menu .cross-btn:after {
    content: ' ';
    position: absolute;
    left: 12px;
    top: 5px;
    height: 24px;
    width: 6px;
    background-color: #093f9c;
    border-radius: 3px;
}
	#mobile-menu .cross-btn:before, #mobile-menu .cross-btn:after {
    content: ' ';
    position: absolute;
    left: 12px;
    top: 5px;
    height: 24px;
    width: 6px;
    background-color: #093f9c;
    border-radius: 3px;
}
	#mobile-menu .cross-btn:before {
    transform: rotate(45deg);
}
	#mobile-menu .cross-btn:after {
    transform: rotate(-45deg);
}
}

