@font-face{
  font-family: "ALSRubl";
  font-style: normal;
  font-weight: normal;
  src: url("../fonts/ALSRubl.eot?") format("eot"),
  url("../fonts/ALSRubl.woff2") format("woff2"),
  url("../fonts/ALSRubl.woff") format("woff"),
  url("../fonts/ALSRubl.ttf") format("truetype"),
  url("../fonts/ALSRubl.svg#str-replace(\"ALSRubl\", \" \", \"_\")") format("svg");
}

