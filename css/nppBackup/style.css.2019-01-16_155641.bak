    height: 9px;
    border-top: 2px solid #ffffff;
    border-left: 2px solid #ffffff;
    position: absolute;
    left: 14px;
    top: 13px;
    transform: rotate(-45deg);
}
.slick-next:before {
    content: '';
    width: 9px;
    height: 9px;
    border-bottom: 2px solid #ffffff;
    border-right: 2px solid #ffffff;
    position: absolute;
    right: 14px;
    top: 13px;
    transform: rotate(-45deg);
}
